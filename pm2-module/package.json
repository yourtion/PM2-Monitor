{
  "name": "pm2-monit-daemon",
  "version": "0.1.0",
  "description": "PM2 Remote Monitor Daemon Module",
  "main": "app.js",
  "dependencies": {
    "pmx": "beta"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com:yourtion/PM2-Monitor.git"
  },
  "config": {
    "send_interval": 30,
    "socket_server": "127.0.0.1:3333"
  },
  "apps": [
    {
      "merge_logs": true,
      "max_memory_restart": "200M",
      "script": "app.js"
    }
  ],
  "author": "Yourtion <yourtion@gmail.com",
  "license": "MIT"
}